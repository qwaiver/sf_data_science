    "holiday_data = pd.read_csv('data/holiday_data.csv', sep=';')\n",
    "\n",
    "def add_holiday_features(taxi_df, holiday_df):\n",
    "    holiday_df['date'] = pd.to_datetime(holiday_df['date'])\n",
    "    holiday_df['date'] = holiday_df['date'].dt.date\n",
    "    merged_df = taxi_df.merge(holiday_df, left_on='pickup_date', right_on='date', how='left')\n",
    "    merged_df = merged_df.fillna(0)\n",
    "    replace_list = list(holiday_df['holiday'])\n",
    "    merged_df['holiday'] = merged_df['holiday'].replace(to_replace=replace_list, value=1)\n",
    "    merged_df['pickup_holiday'] = merged_df['holiday']\n",
    "    merged_df = merged_df.drop(['date', 'day', 'holiday'], axis=1)\n",
    "    return merged_df\n",
    "\n",
    "taxi_hl_data = add_holiday_features(taxi_data, holiday_data)\n",
    "taxi_hl_data.head()\n"
   ]
